# ===== DATABASE CONFIGURATION =====
# Copy this file to .env and fill in your database URLs

# Primary Database (Current - Required)
DATABASE_URL="your_primary_database_url_here"

# Backup Database 1 (Optional - for automatic failover)
DATABASE_URL_BACKUP1="your_backup_database_1_url_here"

# Backup Database 2 (Optional - for additional redundancy)  
DATABASE_URL_BACKUP2="your_backup_database_2_url_here"

# Database Settings
PGDATABASE="your_database_name"
PGHOST="your_database_host"
PGPORT="5432"
PGUSER="your_database_user"
PGPASSWORD="your_database_password"

# ===== APPLICATION CONFIGURATION =====
SESSION_SECRET="your_session_secret_key_here"
NODE_ENV=development
PORT=5000

# ===== FIREBASE CONFIGURATION =====
VITE_FIREBASE_API_KEY="your_firebase_api_key"
VITE_FIREBASE_APP_ID="your_firebase_app_id"
VITE_FIREBASE_PROJECT_ID="your_firebase_project_id"

# ===== CLOUDINARY CONFIGURATION =====
CLOUDINARY_API_KEY="your_cloudinary_api_key"
CLOUDINARY_API_SECRET="your_cloudinary_api_secret"
CLOUDINARY_CLOUD_NAME="your_cloudinary_cloud_name"